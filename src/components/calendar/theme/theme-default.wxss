
/* 日历主要颜色相关样式 */
/*.default_weekend-color*/
/*.default_week-color*/
.default_color,
 {
    color: #1182A4;
}

.default_today {
    color: #fff;
    background-color: #874fb4;
}

.default_choosed {
    color: #fff !important;
    background-color: #1182A4 !important;
}

.default_date-disable {
    color: #c7c7c7;
}

.default_prev-month-date,
.default_next-month-date {
    color: #e2e2e2;
}

.default_normal-date {
    color: #1182A4;
    /* background: #1182A4; */
}

.default_todo-circle {
    border-color: #88d2ac;
}

.default_todo-dot {
    background-color: #e54d42;
}

.default_date-desc {
    color: #c2c2c2;
}

.default_date-desc-lunar {
    color: #e54d42;
}

.default_date-desc-disable {
    color: #e2e2e2;
}
